generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id           String  @id @default(cuid())
  snum         Int     @default(0)
  name         String?
  username     String? @unique
  email        String?
  passwordHash String
  image        String?
  createdby    String?

  @@map("users")
}
